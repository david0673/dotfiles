"*":
  "autocomplete-python":
    useKite: false
  core:
    disabledPackages: [
      "language-python"
    ]
    ignoredNames: [
      ".git"
      ".hg"
      ".svn"
      ".DS_Store"
      "._*"
      "*.pyc"
      "Thumbs.db"
      "desktop.ini"
      ".venv/"
    ]
    projectHome: "/Users/dh/workspace"
    telemetryConsent: "limited"
    themes: [
      "seti-ui"
      "solarized-dark-syntax"
    ]
  editor:
    fontSize: 12
  "exception-reporting":
    userId: "199f5aad-4a7e-02b3-5130-101402f1235d"
  "linter-ui-default":
    panelHeight: 36
  "package-sync":
    createOnChange: true
    forceOverwrite: true
  "tree-view":
    hideIgnoredNames: true
    hideVcsIgnoredFiles: true
  welcome:
    showOnStartup: false
